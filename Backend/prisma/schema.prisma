generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
  
}

model UrlShortener {
  id Int @id @default(autoincrement())
  longUrl String
  shortCode String?
  totalClicks Int @default(0)
  createdAt DateTime @default(now())
  lastClickedAt DateTime @updatedAt
}